<?php

namespace DummyNamespace;

use Tests\TestCase;

class DummyClass extends TestCase
{
    /** @test */
    public function it_can_use_dummy_field(): void
    {
        $response = $this->graphQL(/** @lang GraphQL */ '
            dummyType DummyField {
                dummyField
            }
        ');

        $response->assertJson([
            'data' => [
                'dummyField' => null
            ],
        ]);
    }
}
